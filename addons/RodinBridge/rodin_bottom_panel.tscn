[sub_resource type="GDScript" id="GDScript_1ceok"]
resource_name = "RBTextToBox"
script/source = "@tool extends HBoxContainer

signal option_trigger(value, prop:String)

func _ready() -> void:
	$TextTo.option_trigger.connect(on_text_to_changed.bind(\"textTo\"))
	$CheckBox.pressed.connect(on_bypass_pressed)

func on_text_to_changed(value, prop: String) -> void:
	option_trigger.emit(value, prop)

func on_bypass_pressed() -> void:
	option_trigger.emit($CheckBox.button_pressed, \"bypass\")
"

