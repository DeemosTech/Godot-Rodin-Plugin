[sub_resource type="GDScript" id="GDScript_k65g3"]
resource_name = "DirectTo3D"
script/source = "@tool 
class_name DirectTo3D extends CheckBox

func _ready() -> void:
	%TextToBox/TextTo.option_selected.connect(display_cb)
	%GenTypeOption.item_selected.connect(disable_cb)

func display_cb(btn: Button) -> void:
	visible = btn.name == \"Text\"
	var id = %GenTypeOption.selected
	disable_cb(id)

func disable_cb(id) -> void:
	disabled = %GenTypeOption.get_item_text(id) == \"OneClick\"
"

[sub_resource type="GDScript" id="GDScript_ejkgi"]
resource_name = "TextInput"
script/source = "@tool
class_name TextInput extends LineEdit

signal option_trigger(value, prop:String)

func _ready() -> void:
	%TextToBox/TextTo.option_selected.connect(display_cb)
	self.text_changed.connect(on_prompt_chanaged)

func on_prompt_chanaged(value: String) -> void:
	option_trigger.emit(value, \"text_input\")

func display_cb(btn: Button) -> void:
	visible = btn.name == \"Text\"
"

[sub_resource type="GDScript" id="GDScript_y06av"]
resource_name = "RBComptibleBrowerError"
script/source = "@tool
class_name RBCompatibleBrowerError extends HBoxContainer

func _ready() -> void:
	var theme: Theme = RBUtils.get_editor_interface().get_editor_theme()
	$Icon.texture = theme.get_icon(\"StatusError\", \"EditorIcons\")

func update() -> void:
	if RBWSServer.get_compatible_browser():
		self.show()
	else:
		self.hide()
"

