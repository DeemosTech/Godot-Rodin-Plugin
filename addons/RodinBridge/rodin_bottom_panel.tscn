[sub_resource type="GDScript" id="GDScript_rjsy7"]
resource_name = "RBGenTypeOption"
script/source = "@tool extends OptionButton

signal option_trigger(value, prop:String)

func _ready() -> void:
	self.item_selected.connect(on_gen_type_changed)
	var timer = Timer.new()
	timer.wait_time = 1
	timer.autostart = true
	timer.timeout.connect(force_gen_type_oneclick)
	add_child(timer)

func on_gen_type_changed(i: int) -> void:
	var value = self.get_item_text(i)
	option_trigger.emit(value, \"gen_type\")

func force_gen_type_oneclick() -> void:
	var i = self.get_selected_id()
	if RBWSServer.get_compatible_browser() and self.get_item_text(i) != \"OneClick\":
		self.select(0)
		self.item_selected.emit(0)
"

[sub_resource type="GDScript" id="GDScript_o4pcp"]
resource_name = "RBGenTypeBox"
script/source = "@tool
class_name RBGenTypeBox extends VBoxContainer

signal option_trigger(value, prop:String)

func _ready() -> void:
	%GenTypeOption.item_selected.connect(display_cb)
	%PolygonsOption.option_trigger.connect(on_polygons_changed)
	$Quality.value_changed.connect(on_quality_changed)
	
func on_polygons_changed(value) -> void:
	option_trigger.emit(value, \"polygons\")

func on_quality_changed(value: float) -> void:
	option_trigger.emit(value, \"quality\")

func display_cb(i: int) -> void:
	visible = %GenTypeOption.get_item_text(i) == \"OneClick\"
"

