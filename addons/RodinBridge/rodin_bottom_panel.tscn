[sub_resource type="GDScript" id="GDScript_kh3wh"]
resource_name = "RBImageBox"
script/source = "@tool extends VBoxContainer

signal option_trigger(value, prop:String)

func _ready() -> void:
	$HBoxContainer/AlignOption.item_selected.connect(on_align_option_selected)
	%ConditionOption.option_selected.connect(display_cb)
	$Height.value_changed.connect(on_height_changed)

func on_align_option_selected(i: int) -> void:
	var value = $HBoxContainer/AlignOption.get_item_text(i)
	option_trigger.emit(value, \"align\")

func on_height_changed(value: float) -> void:
	option_trigger.emit(value, \"height\")

func display_cb(btn: Button) -> void:
	visible = btn.name == \"image\"
"

