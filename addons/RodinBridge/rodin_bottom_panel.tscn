[sub_resource type="GDScript" id="GDScript_s6rj1"]
resource_name = "RBMatResolution"
script/source = "@tool 
class_name RBMatResolution extends OptionButton

func _ready() -> void:
	%ModeOption.option_selected.connect(display_cb)

func display_cb(btn: Button) -> void:
	var id = self.get_selected_id()
	while self.item_count > 0:
		self.remove_item(self.item_count - 1)
	var items = [\"1K\"] if btn.name == \"Fast\" else [\"2K\", \"4K\"]
	for i in range(items.size()):
		self.add_item(items[i], i)
	self.select(mini(id, self.item_count - 1))
"

